{
  "name": "animated-sansa",
  "version": "0.1.0",
  "description": "A game about avoiding your past self over a crushing future",
  "main": "src/animated-sansa.js",
  "scripts": {
    "start": "budo src/animated-sansa.js:js/animated-sansa.js --live | opnr | garnish",
    "format": "standard --format src/*.js",
    "build": "browserify src/animated-sansa.js | uglifyjs -cm --screwie8 > js/animated-sansa.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustine/animated-sansa.git"
  },
  "keywords": [
    "game",
    "timetravel",
    "arcade"
  ],
  "author": "Dustine Camacho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustine/animated-sansa/issues"
  },
  "homepage": "https://github.com/dustine/animated-sansa#readme",
  "dependencies": {},
  "devDependencies": {
    "craftyjs": "^0.6.3",
    "eslint": "^0.24.0",
    "eslint-config-standard": "^3.4.1",
    "opnr": "^1.0.4",
    "standard": "^4.5.2",
    "uglify-js": "^2.4.23"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
